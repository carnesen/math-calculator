{
  "name": "@carnesen/mathjs-app",
  "description": "A toy calculator web app built on math.js",
  "version": "0.0.0",
  "main": "lib/index.js",
  "scripts": {
    "build:browser:min": "cross-env NODE_ENV=production npm run build:browser",
    "build:browser": "cross-env BABEL_ENV=browser webpack --bail",
    "build:browser:watch": "npm run build:browser -- --watch",
    "build:server": "cross-env BABEL_ENV=server babel src --out-dir lib --ignore *-test.js,browser",
    "build:server:watch": "npm run build:server -- --watch",
    "build": "npm run clean && npm run build:server",
    "clean": "rimraf lib dist",
    "lint": "standard 'src/**/*.js' webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=production node lib/server/start.js",
    "start:dev": "cross-env DEBUG=@carnesen/* nodemon --watch lib lib/server/start.js",
    "test": "npm run lint && npm run test:server",
    "test:server": "cross-env BABEL_ENV=server NODE_ENV=test mocha --compilers js:babel-register src/server/**/*-test.js src/shared/**/*-test.js"
  },
  "dependencies": {
    "@carnesen/logger": "0.0.1",
    "@carnesen/redux-add-action-listener-enhancer": "0.0.1",
    "@carnesen/redux-sliced": "0.0.2",
    "body-parser": "1.15.2",
    "cross-env": "3.0.0",
    "express": "4.14.0",
    "mathjs": "3.5.3",
    "morgan": "1.7.0",
    "socket.io": "1.4.8",
    "socket.io-client": "1.4.8",
    "webpack": "1.13.2"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "json-loader": "0.5.4",
    "mocha": "3.1.0",
    "nodemon": "1.10.2",
    "rimraf": "2.5.4",
    "standard": "8.3.0",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0",
    "webpack-node-externals": "1.4.3"
  },
  "files": [
    "lib"
  ],
  "author": {
    "name": "Chris Arnesen",
    "email": "chris.arnesen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carnesen/mathjs-app.git"
  },
  "bugs": {
    "url": "https://github.com/carnesen/mathjs-app/issues"
  },
  "homepage": "https://github.com/carnesen/mathjs-app",
  "license": "MIT",
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
