{
  "name": "@carnesen/mathjs-app",
  "description": "A toy calculator web app built on math.js",
  "version": "0.0.0",
  "main": "lib/server/index.js",
  "scripts": {
    "build:browser": "cross-env BABEL_ENV=browser webpack",
    "build:browser:min": "cross-env NODE_ENV=production npm run build:browser -- --bail",
    "build:browser:watch": "npm run build:browser -- --watch",
    "build:server": "cross-env BABEL_ENV=server babel src --out-dir lib --ignore *-test.js,browser",
    "build:server:watch": "npm run build:server -- --watch",
    "build": "npm run clean && npm run build:server && npm run build:browser && npm run build:browser:min",
    "clean": "rimraf lib dist",
    "lint": "eslint src webpack.config.babel.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "node bin/www.js",
    "start:dev": "cross-env DEBUG=@carnesen/* nodemon --watch lib bin/www.js",
    "test": "npm run lint && npm run build && npm run test:server && npm run test:browser",
    "test:browser": "cross-env BABEL_ENV=browser NODE_ENV=test mocha --require .before-browser-tests.js src/browser/**/*-test.js src/shared/**/*-test.js",
    "test:server": "cross-env BABEL_ENV=server NODE_ENV=test mocha --require .before-server-tests.js src/server/**/*-test.js src/shared/**/*-test.js"
  },
  "dependencies": {
    "@carnesen/checks": "0.0.1",
    "@carnesen/logger": "0.0.1",
    "@carnesen/redux-add-action-listener-enhancer": "0.0.1",
    "@carnesen/redux-sliced": "0.0.3",
    "@carnesen/util": "0.0.7",
    "body-parser": "1.15.2",
    "cross-env": "3.1.2",
    "express": "4.14.0",
    "mathjs": "3.5.3",
    "morgan": "1.7.0",
    "octicons": "4.4.0",
    "socket.io": "1.5.0",
    "uuid": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-config-carnesen": "1.0.4",
    "json-loader": "0.5.4",
    "material-ui": "0.16.1",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-tap-event-plugin": "1.0.0",
    "rimraf": "2.5.4",
    "socket.io-client": "1.5.0",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0",
    "webpack": "1.13.2",
    "whatwg-fetch": "1.0.0"
  },
  "files": [
    "dist",
    "lib",
    "public"
  ],
  "author": {
    "name": "Chris Arnesen",
    "email": "chris.arnesen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carnesen/mathjs-app.git"
  },
  "bugs": {
    "url": "https://github.com/carnesen/mathjs-app/issues"
  },
  "homepage": "https://github.com/carnesen/mathjs-app",
  "license": "MIT"
}
